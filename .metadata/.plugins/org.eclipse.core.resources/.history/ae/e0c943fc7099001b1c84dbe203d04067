package ejercicios;


import java.util.Scanner;

public class Main {
	
	

	private static String[] getRandomPalabras(String[] palabras) {
		
		Scanner input = new Scanner(System.in);
		
		for(int i=0;i<10;i++) {
			palabras[i] = input.nextLine();		
			};
		
		return palabras;
	};
	
	public static void main(String[] args) {
		
		
		String[] palabras = new String[10];
		palabras = getRandomPalabras(palabras);
		
		

	}

}
